import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner n=new Scanner(System.in);
		int X=n.nextInt();
		int Y=n.nextInt();
		if(X<Y)
		{
		        System.out.println(X);
		    }
		else
		{
		    System.out.println(Y);
		}
		}
}
