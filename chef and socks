import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	Scanner n=new Scanner(System.in);
	int A=n.nextInt();
	int X=n.nextInt();
	int Y=n.nextInt();
	if(A<(X+Y))
	    System.out.println("YES");
	else
	    System.out.println("NO");

	}
}
